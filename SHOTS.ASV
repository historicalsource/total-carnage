	CALLR	CHK_BULL
	JRNC	NOH
;HIT
	CLR	A0
	MOVE	A0,*A8(OXVEL),L
	MOVE	A0,*A8(OYVEL),L
	MOVK	1,A1
	JSRP	FRANIM
C32X	MOVE	A8,A0
	CALLA	DELOBJ
	DIE
NOH	CLR	A1
	JSRP	FRANIM
	JRC	C32X
	JRUC	W3_TP

WPN4:
