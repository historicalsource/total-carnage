	.FILE	'SHOTS.ASM'
	.TITLE	'PROJECTILES FROM PLAYERS STUFF'
	.WIDTH	132
	.OPTION	B,D,L,T
	.MNOLIST
	
;
; INITIATED:		 AUG 30,1990
; MODIFIED:		 !
; SOFTWARE:		 MARK TURMELL
;
