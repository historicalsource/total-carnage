STRT_WPN:
;FROM TORSO PROCESS
	MOVE	*A13(PNUM),A11		;PLAYER WHO FIRED ME PASSED IN A11
	MOVE	*A13(WPN_TYP),A1,W
	JRZ	CONTA
;ANY WEAPON BUT STANDARD WILL DEC SHOTCNT
	MOVE	*A13(SHOTCNT),A0
	DEC	A0
	MOVE	A0,*A13(SHOTCNT)
	JRNZ	CONTA
	MOVE	A0,*A13(WPN_TYP)
CONTA
	SLL	5,A1
	ADDI	WPLIST,A1
	MOVE	*A1,A0,L
	JUMP	A0

